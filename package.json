{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch:css": "onchange \"src/theme\" -- npm run build:css",
    "watch:js": "onchange \"src/pages/**/*.js\" -- npm run build:local-js",
    "watch:all": "concurrently --kill-others 'npm run watch:js' 'npm run watch:css'",
    "clean": "rm -r public/*",
    "fonts": "cp -r static/fonts public/fonts",
    "autoprefixer": "postcss -u autoprefixer -r public/stylesheets/*",
    "build:vendor-css": "node-sass --output-style compressed -o public/stylesheets static/vendor.scss",
    "build:css": "node-sass --output-style compressed -o public/stylesheets src/theme/style.scss",
    "build:images": "npm run imagemin",
    "imagemin": "imagemin static/images/* -o public/images",
    "build:vendor-js": "browserify static/vendor.js > public/javascripts/vendor.js",
    "build:local-js": "gulp build-dev",
    "build:prod-js": "gulp build-prod",
    "build:local-webpacker": "gulp local-webpacker",
    "build:prod-webpacker": "gulp prod-webpacker",
    "prebuild": "mkdirp public/javascripts/client",
    "build:all": "npm run clean && npm run prebuild && npm run build:vendor-css && npm run build:css && npm run build:images && npm run build:vendor-js",
    "build-dev": "npm run build:all && npm run build:local-js && npm run watch:all",
    "build-prod": "npm run build:all && npm run build:prod-js"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "cookies-js": "^1.2.2",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.13.4",
    "morgan": "~1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "browserify": "^13.1.0",
    "concurrently": "^2.2.0",
    "config": "^1.21.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-tap": "^0.1.3",
    "gulp-util": "^3.0.7",
    "gulp-wait": "0.0.2",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "install": "^0.8.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.0",
    "npm": "^3.10.8",
    "onchange": "^3.0.2",
    "path": "^0.12.7",
    "pretty-hrtime": "^1.0.2",
    "run-sequence": "^1.2.2",
    "string-replace-loader": "^1.0.5",
    "strip-loader": "^0.1.2",
    "webpack": "^2.1.0-beta.25"
  }
}
